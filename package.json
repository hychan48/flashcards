{
  "name": "flashcards",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "cross-env NUXT_ENV_ROUTER_BASE='/flashcards/' NUXT_ENV_BASE_URL='https://hychan48.github.io/' nuxt generate",
    "generate2": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "minify-static": "html-minifier --input-dir ./static/flashcards/gen --output-dir ./static/flashcards/static --collapse-whitespace --continue-on-parse-error --case-sensitive",
    "minify-static_dist_test": "html-minifier --input-dir ./dist/flashcards/gen --output-dir ./dist/test --collapse-whitespace --continue-on-parse-error",
    "test1": "html-minifier --version",
    "test": "html-minifier"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "core-js": "^3.18.0",
    "nuxt": "^2.15.8",
    "package.json": "^2.0.1"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^1.11.3",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "esm": "^3.2.25",
    "html-minifier": "^4.0.0",
    "mocha": "^9.1.1",
    "module-alias": "^2.2.2",
    "push-dir": "^0.4.1",
    "yarn-audit-fix": "^9.3.2"
  },







  "_moduleAliases": {
    "@": ".",
    "~": "."
  }
}
